{
 "extends": ["config:base"],
 "baseBranches": ["master"],
 "bumpVersion": null,
 "packageRules": [
   {
     "depTypeList": ["devDependencies"],
     "updateTypes": ["minor"],
     "automerge": false
   },
   {
      "matchPackagePatterns": ["docker.io/calico/cni"],
      "matchCurrentVersion": "/^v\\./"
   }
 ],
 "prHourlyLimit": 5,
 "prConcurrentLimit": 10,
 "rangeStrategy": "bump",
 "renovateFork": true,
 "includeForks": true,
 "regexManagers": [
    {
      "fileMatch": ["target_cluster_container_images.sh$"],
      "matchStrings": ["docker.io/calico/cni:(?<currentValue>.*?)"],
      "depNameTemplate": "calico/cni",
      "datasourceTemplate": "docker"
    }
  ]
}
